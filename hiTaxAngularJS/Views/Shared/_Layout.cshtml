						<span class="menu-text"> Dashboard </span>
					</a>
					<b class="arrow"></b>
				</li>
				<li class="@(ViewContext.RouteData.Values["Controller"].ToString() + ViewContext.RouteData.Values["Action"].ToString() == "InvoiceInputInvoiceInputV2" ? "active" : "")">
					<a href="/InvoiceInput/InvoiceInputV2">
						<i class="menu-icon fa fa-indent"></i>
						<span class="menu-text"> Invoice Input</span>
					</a>

					<b class="arrow"></b>
				</li>
				<li class="@(ViewContext.RouteData.Values["Controller"].ToString() + ViewContext.RouteData.Values["Action"].ToString() == "InvoicesIndex" ? "active" : "")">

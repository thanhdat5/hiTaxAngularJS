                                 ? "open active" : "")">
					<a href="#" class="dropdown-toggle">
						<i class="menu-icon fa fa-group"></i>
						<span class="menu-text"> Customers </span>

						<b class="arrow fa fa-angle-down"></b>
					</a>

					<b class="arrow"></b>

					<ul class="submenu">
						<li class="@(ViewContext.RouteData.Values["Controller"].ToString() == "Customers"? "active" : "")">
							<a href="/Customers">
								<i class="menu-icon fa fa-caret-right"></i>
								Customer management
							</a>

							<b class="arrow"></b>
						</li>

						<li class="@(ViewContext.RouteData.Values["Controller"].ToString() == "CustomerTypes"? "active" : "")">
							<a href="/CustomerTypes">
								<i class="menu-icon fa fa-caret-right"></i>
								Customer Types
							</a>

							<b class="arrow"></b>
						</li>

						<li class="@(ViewContext.RouteData.Values["Controller"].ToString() == "Companies"? "active" : "")">
							<a href="/Companies">
								<i class="menu-icon fa fa-caret-right"></i>
								Companies
							</a>

							<b class="arrow"></b>
						</li>

						<li class="@(ViewContext.RouteData.Values["Controller"].ToString() == "Departments"? "active" : "")">
							<a href="/Departments">
								<i class="menu-icon fa fa-caret-right"></i>
								Departments
							</a>

							<b class="arrow"></b>
						</li>
					</ul>
				</li>
				<li class="@(ViewContext.RouteData.Values["Controller"].ToString() == "Categories"
                             || ViewContext.RouteData.Values["Controller"].ToString() == "TaxValues"
                             || ViewContext.RouteData.Values["Controller"].ToString() == "Types"
                             || ViewContext.RouteData.Values["Controller"].ToString() == "Units"
                             || ViewContext.RouteData.Values["Controller"].ToString() == "Settings"
                                 ? "open active" : "")">

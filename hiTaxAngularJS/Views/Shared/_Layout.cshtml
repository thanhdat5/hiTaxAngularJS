	<script src="/Content/Template/ace-master/assets/js/html5shiv.min.js"></script>
	<script src="/Content/Template/ace-master/assets/js/respond.min.js"></script>
	<![endif]-->
	<link href="/Content/Template/toast/css/jquery.toast.css" rel="stylesheet" />
	<style>
		.oht-th-id {
			width: 40px;
		}

		.oht-th-action {
			width: 90px;
		}

		.oht-edit-mode {
			display: none;
		}

		.profile-info-name {
			width: 140px;
		}
	</style>
	@RenderSection("styles", required: false)
</head>

<body class="no-skin">

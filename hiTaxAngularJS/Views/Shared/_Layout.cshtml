@{
	Layout = null;
	bool isLogged = (System.Web.HttpContext.Current.User != null) && System.Web.HttpContext.Current.User.Identity.IsAuthenticated;
	if (!isLogged)
	{
		Response.Redirect("/Account/Login");
	}
}
<!DOCTYPE html>
<html lang="en">
<head>
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
	<meta charset="utf-8" />
	<title>@ViewBag.Title - OHT Tax Support Application</title>

	<meta name="description" content="" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
	<!-- bootstrap & fontawesome -->
	<link rel="stylesheet" href="/Content/Template/ace-master/assets/css/bootstrap.min.css" />
	<link href="/Content/font-awesome.min.css" rel="stylesheet" />

	<!-- page specific plugin styles -->
	<!-- text fonts -->
	<link rel="stylesheet" href="/Content/Template/ace-master/assets/fonts/fonts.googleapis.com.css" />

	<!-- ace styles -->
	<link rel="stylesheet" href="/Content/Template/ace-master/assets/css/ace.min.css" class="ace-main-stylesheet" id="main-ace-style" />

	<!--[if lte IE 9]>
		<link rel="stylesheet" href="/Content/Template/ace-master/assets/css/ace-part2.min.css" class="ace-main-stylesheet" />
	<![endif]-->
	<!--[if lte IE 9]>
